{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": {
      "options": {
        "multiline": "never",
        "singleline": "never"
      }
    },
    "quotemark": {
      "options": [
        "single",
        "avoid-escape"
      ]
    },
    "max-line-length": [true, 180]
  },
  "rulesDirectory": []
}